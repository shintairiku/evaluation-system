# envの詳細はNotion（インターン >> 細々とした情報）にて確認

# Demo environment configuration for testing
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=""
CLERK_SECRET_KEY=""
CLERK_WEBHOOK_URL=""
CLERK_WEBHOOK_SECRET=""

# JWT Verification (for organization-scoped security)
# CLERK_ISSUER: Your Clerk Frontend API URL (e.g., https://your-app.clerk.accounts.dev)
# Find this in Clerk Dashboard > Settings > API Keys > Frontend API
CLERK_ISSUER=""

# CLERK_AUDIENCE: Your application identifier for JWT audience validation
# This can be your domain name or app identifier (e.g., "localhost:3000" for dev, "yourapp.com" for prod)
CLERK_AUDIENCE=""

# Organization Features
# Set to "true" to enable organization-scoped routing and security
CLERK_ORGANIZATION_ENABLED=true


# Supabase (minimal config)
NEXT_PUBLIC_SUPABASE_URL=""
NEXT_PUBLIC_SUPABASE_ANON_KEY=""
SUPABASE_DATABASE_URL=""

# Application Configuration
DEBUG=True
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Logging Configuration
LOG_LEVEL=INFO